<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

<!-- Servlet index -->
	<servlet>
		<servlet-name>HexoSkin_Dashboard_GAE</servlet-name>
		<servlet-class>servlets_GAE.HexoSkin_Dashboard_Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HexoSkin_Dashboard_GAE</servlet-name>
		<url-pattern>/dashboard</url-pattern>
	</servlet-mapping>
	
<!-- Servlet compare -->
	<servlet>
		<servlet-name>HexoSkin_Compare_GAE</servlet-name>
		<servlet-class>servlets_GAE.HexoSkin_Compare_Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HexoSkin_Compare_GAE</servlet-name>
		<url-pattern>/compare</url-pattern>
	</servlet-mapping>
	
	
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	

	
</web-app>
